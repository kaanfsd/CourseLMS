                        {
                            <li class="nav-item">
                                <a class="nav-link text-white" asp-area="" asp-controller="Courses" asp-action="Index">Course</a>
                            </li>
                        }
                        @if (User.IsInRole(StaticDetail.Role_Admin))
                        {
